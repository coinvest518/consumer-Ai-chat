{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "ASTRA_DB_APPLICATION_TOKEN": "@astra_db_application_token",
    "ASTRA_DB_ENDPOINT": "@astra_db_endpoint",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key",
    "FLOW_ID": "@flow_id"
  }
}
